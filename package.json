{
  "name": "@icw/icw",
  "version": "0.1.0",
  "description": "Stream-like async iterable utility library",
  "license": "0BSD",
  "author": "Benjamin Chauvette <bdchauvette@gmail.com>",
  "homepage": "https://github.com/icwjs/icw#readme",
  "repository": "github:icwjs/icw",
  "bugs": "https://github.com/icwjs/icw/issues",
  "keywords": [
    "async",
    "asynchronous",
    "iteration",
    "iterator",
    "iterable",
    "stream",
    "reactive",
    "frp"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "cjs/index",
  "module": "index",
  "dependencies": {},
  "optionalDependencies": {
    "@types/node": "^10.5.4"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.56",
    "@commitlint/cli": "7.0.0",
    "@commitlint/config-conventional": "7.0.1",
    "@types/jest": "23.3.1",
    "cross-env": "5.2.0",
    "eslint": "5.3.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-prefer-let": "1.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-typescript": "0.12.0",
    "husky": "1.0.0-rc.13",
    "jest": "23.4.2",
    "lint-staged": "7.2.0",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.0",
    "ts-jest": "23.1.3",
    "typescript": "3.0.1",
    "typescript-eslint-parser": "18.0.0"
  },
  "buildDependencies": [
    "npm-run-all",
    "rimraf",
    "typescript"
  ],
  "scripts": {
    "clean": "node scripts/clean",
    "prebuild": "npm run clean",
    "build": "npm-run-all --parallel build:*",
    "build:esm": "tsc -p tsconfig.prod.json --module es2015 --outDir .",
    "build:cjs": "tsc -p tsconfig.prod.json --module commonjs --outDir cjs",
    "test": "jest",
    "lint": "npm-run-all lint:eslint \"lint:prettier -- --list-different\"",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint:prettier": "prettier \"./**/*.{json,md,yml}\"",
    "lint:fix": "npm-run-all \"lint:eslint -- --fix\" \"lint:prettier -- --write\"",
    "prepare": "npm run build",
    "postpack": "npm run clean"
  }
}
