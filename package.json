{
  "name": "@icw/icw",
  "version": "0.1.0",
  "description": "Async iteration utility library",
  "license": "0BSD",
  "author": "Benjamin Chauvette <bdchauvette@gmail.com>",
  "homepage": "https://github.com/icwjs/icw#readme",
  "repository": "github:icwjs/icw",
  "bugs": "https://github.com/icwjs/icw/issues",
  "keywords": [
    "async",
    "asynchronous",
    "frp",
    "iterable",
    "iteration",
    "iterator",
    "promise",
    "reactive",
    "stream",
    "utility"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "cjs/index",
  "module": "index",
  "sideEffects": false,
  "dependencies": {},
  "optionalDependencies": {
    "@types/node": "^10.5.4"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@types/jest": "23.3.9",
    "agadoo": "1.0.1",
    "cross-env": "5.2.0",
    "eslint": "5.9.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.0.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-prefer-let": "1.0.1",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-typescript": "0.13.0",
    "husky": "1.2.0",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "npm-run-all": "4.1.3",
    "plop": "2.1.0",
    "prettier": "1.15.2",
    "semver": "5.6.0",
    "ts-jest": "23.10.4",
    "typescript": "3.1.6",
    "typescript-eslint-parser": "21.0.1"
  },
  "scripts": {
    "// Utility ----------------------------------------------------------": "",
    "check-node-version": "node scripts/check-node-version",
    "// Building ---------------------------------------------------------": "",
    "prebuild": "npm run clean",
    "build": "npm-run-all --parallel build:*",
    "build:esm": "tsc -p tsconfig.prod.json --module es2015 --outDir .",
    "build:cjs": "tsc -p tsconfig.prod.json --module commonjs --outDir cjs",
    "preclean": "npm run check-node-version",
    "clean": "cross-env NODE_NO_WARNINGS=1 node scripts/clean",
    "// Linting ----------------------------------------------------------": "",
    "lint": "npm-run-all --parallel lint:eslint \"lint:prettier -- --list-different\"",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint:prettier": "prettier \"./**/*.{json,md,yml}\"",
    "lint:fix": "npm-run-all \"lint:eslint -- --fix\" \"lint:prettier -- --write\"",
    "// Scaffolding ------------------------------------------------------": "",
    "preplop": "npm run check-node-version",
    "plop": "cross-env NODE_NO_WARNINGS=1 plop",
    "// Testing ----------------------------------------------------------": "",
    "pretest": "npm run check-node-version",
    "test": "jest",
    "// Publishing -------------------------------------------------------": "",
    "prepare": "npm run build",
    "prepublishOnly": "agadoo",
    "postpack": "npm run clean"
  }
}
